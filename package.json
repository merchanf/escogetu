{
  "name": "escogetu",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "contributors": [],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "yarn build:prepare && react-app-rewired build",
    "build:prepare": "rimraf ./build",
    "lint": "stylelint \"src/**/*.{scss,css}\" && eslint ./src --ext=.js,.ts,.jsx --max-warnings=0",
    "lint:fix": "stylelint \"src/**/*.{scss,css}\" --fix && eslint ./src --ext=.js,.ts,.jsx --fix --max-warnings=0",
    "lint:ci:css_base": "stylelint \"./src/**/*.{scss,css}\" --max-warnings 0 --cache --cache-location \"./.stylelintcache/\"",
    "lint:ci:css": "yarn lint:ci:css_base -f json -o ./stylelint-output.json > /dev/null || yarn lint:ci:css_base",
    "lint:ci": "yarn lint:ci:css && yarn eslint-output --max-warnings=0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@firebase/analytics": "^0.6.2",
    "@googlemaps/js-api-loader": "^1.11.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.21.1",
    "axios-hooks": "^2.3.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "dotenv": "^8.2.0",
    "firebase": "^8.2.7",
    "google-map-react": "^2.1.9",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-morphing-modal": "^0.2.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-tinder-card": "^1.3.1",
    "sass": "^1.32.6",
    "uid": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "babel-eslint": "10.1.0",
    "customize-cra": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-problems": "^3.1.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-output": "^2.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
    "esm": "^3.2.25",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "react-app-rewired": "^2.1.6",
    "react-refresh": "^0.8.3",
    "react-scripts": "^3.4.3",
    "rimraf": "^3.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.17.1"
  },
  "config-overrides-path": "./config/cra-overrides.js"
}
